<odoo>
    <data>
        <record id="state_property_list_view" model="ir.ui.view">
        <field name="name">state.property.list</field>
        <field name="model">state.property</field>
        <field name="arch" type="xml">
            <list string="State Properties">
                <field name="name"/>
                <field name="postcode"/>
                <field name="bedrooms"/>
                <field name="living_area"/>
                <field name="expected_price"/>
                <field name="selling_price"/>
                <field name="date_availability"/>
                <field name="postcode"/>
            </list>
        </field>
        </record>
    </data>
</odoo>